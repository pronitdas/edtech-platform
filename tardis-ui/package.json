    "globals": "^17.2.0",
    "happy-dom": "^20.4.0",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.3",
    "prettier": "^3.8.1",
