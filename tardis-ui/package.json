    "openai": "^6.16.0",
    "p5": "^2.2.0",
    "pdfjs-dist": "^5.4.530",
    "puppeteer": "^24.36.1",
    "react": "^19.2.4",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.4",
    "react-markdown": "^10.1.0",
    "react-player": "^3.4.0",
    "react-router-dom": "^7.13.0",
    "recharts": "^3.7.0",
    "rehype": "^13.0.2",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-rehype": "^11.1.2",
    "tailwind-merge": "^3.4.0",
