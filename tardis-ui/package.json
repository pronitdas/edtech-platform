    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@shadcn/ui": "^0.0.4",
    "@types/react": "^19.2.10",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@xyflow/react": "^12.10.0",
    "axios": "^1.13.4",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
